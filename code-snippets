print("Automating with Python is fun!")

sec_per_day = 86400
day_per_week = 7
sec_per_week = sec_per_day/day_per_week
print(sec_per_week)

possible_password = 26
print(possible_password ** 6)

disk_size = 16*1024*1024*1024
sector_size = 512
sector_amount = disk_size/sector_size

print(sector_amount)
